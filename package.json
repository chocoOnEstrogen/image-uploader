		"copy": "node ./scripts/copy.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/stellaOnEstrogen/image-uploader.git"
	},
	"keywords": [],
	"author": "stellaOnEstrogen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stellaOnEstrogen/image-uploader/issues"
	},
	"homepage": "https://github.com/stellaOnEstrogen/image-uploader#readme",
	"files": [
		"bin/",
		"src/",
		"scripts/",
		"docs/"
	],
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^4.17.21",
		"@types/express-session": "^1.18.0",
		"@types/jest": "^29.5.12",
		"@types/multer": "^1.4.11",
		"@types/node": "^22.0.0",
		"@types/node-cron": "^3.0.11",
		"@types/ws": "^8.5.12",
		"jest": "^29.7.0",
		"prettier": "^3.3.3",
		"prettier-plugin-ejs": "^1.0.3",
		"ts-jest": "^29.2.3",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.5.4"
