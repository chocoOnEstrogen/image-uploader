		"dotenv": "^16.4.5",
		"ejs": "^3.1.9",
		"express": "^4.19.2",
		"express-rate-limit": "^7.4.0",
		"express-session": "^1.18.0",
		"module-alias": "^2.2.3",
		"multer": "^1.4.5-lts.1",
