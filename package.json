{
	"name": "image-uploder",
	"version": "1.0.0-rc.1",
	"description": "A simple image uploader",
	"bin": {
		"image-uploder": "./bin/app.js"
	},
	"main": "./bin/app.js",
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"format": "prettier --cache --write . --ignore-path .prettierignore --plugin=prettier-plugin-ejs",
		"build": "tsc -p tsconfig.json",
		"prepublishOnly": "npm run build",
		"test": "jest",
